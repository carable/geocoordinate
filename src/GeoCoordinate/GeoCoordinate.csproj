<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <IsWindows Condition="'$(OS)' == 'Windows_NT'">true</IsWindows>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework Condition=" '$(IsWindows)' == ''">netstandard1.4</TargetFramework>
    <TargetFrameworks Condition=" '$(IsWindows)' == 'true'">netstandard1.4;net45</TargetFrameworks>
    <Version>2.0.0</Version>
    <PackageId>Carable.GeoCoordinate</PackageId>
    <RootNamespace>Carable.GeoCoordinates</RootNamespace>
    <AssemblyName>Carable.GeoCoordinate</AssemblyName>
    <Authors>GeoCoordinate</Authors>
